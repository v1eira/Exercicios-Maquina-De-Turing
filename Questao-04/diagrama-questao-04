digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	accept
	node [shape=circle]
	q0 -> q1 [label = "1/y r"]
	q0 -> accept [label = "x/x r"]
	
	q1 -> q1 [label = "*/* r"]
	q1 -> q2 [label = "x/x r"]
	
	q2 -> q3 [label = "1/z r"]
	q2 -> q5 [label = "=/= l"]
	
	q3 -> q3 [label = "*/* r"]
	q3 -> q4 [label = "_/1 l"]
	
	q4 -> q4 [label = "*/* l"]
	q4 -> q2 [label = "z/1 r"]
	
	q5 -> q5 [label = "*/* l"]
	q5 -> q0 [label = "y/1 r"]
}
